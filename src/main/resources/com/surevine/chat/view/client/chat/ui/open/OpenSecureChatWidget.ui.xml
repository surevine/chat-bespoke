	xmlns:s="urn:import:com.surevine.chat.view.client.security.labelchooser"
	xmlns:c="urn:import:com.surevine.chat.view.client.security.labelchooser.caveats"
	xmlns:security="urn:import:com.surevine.chat.view.client.security.ui">
	<ui:style>
		
	</ui:style>
	<g:ScrollPanel ui:field="scroll" styleName="scroll">
